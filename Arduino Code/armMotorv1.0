#include <Servo.h>

Servo m1;

void setup() {
  m1.attach(6);
  Serial.begin(9600);
  delay(50);
  Serial.println("Serial ready:");
  arm(m1);
}

void loop() {

  Serial.println("Main loop entered")
  m1.write(1500);
  Serial.println(1500);
  delay(1200)
  m1.write(1900)
  delay(1500)
  m1.write(1000)
  delay(1000)
}

void arm(Servo m){
  Serial.print("Arming.");
  m.write(0);
  delay(100);
  m.write(1224); //A value at which the motor starts turning
  delay(2000);
  m.write(1024); //A value at which the motor stands still
  delay(3000);
  Serial.println(" Armed!");
}